<div id="landing" class="projects-container">
	<ul>
		<li id="book-cover" class="cd-single-project">
			<div class="cd-title">
				<img src="/assets/images/titulo-libro.svg" alt="Tenemos Patria, Tenemos Proyecto, Somos Futuro">
			</div> <!-- .cd-title -->
		</li>
		<li id="book-menu" class="cd-single-project effect">
			<div class="cd-title">
				<ul class="home-politicas">
				{{#each model.items as |menuItem|}}
					{{#unless menuItem.parent}}
						{{wp-menu-item item=menuItem}}
					{{/unless}}
				{{/each}}
				</ul>
			</div> <!-- .cd-title -->
		</li>
	</ul>
</div> <!-- .project-container -->
